{
    "name": "code-sum-up",
    "displayName": "code-sum-up",
    "description": "Sum up numbers in selection",
    "version": "0.1.0",
    "publisher": "arturbelico",
    "engines": {
        "vscode": "^1.23.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/arturcarvalho/code-sum-up"
    },
    "categories": [
        "Other"
    ],
