{
	"name": "tablegenerator",
	"displayName": "Markdown Table Generator",
	"description": "Generate formatted tables for markdown",
	"version": "1.0.4",
	"publisher": "JayFiDev",
	"engines": {
		"vscode": "^1.24.0"
	},
	"categories": [
		"Other",
		"Programming Languages"
	],
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#0A6A6A",
		"theme": "dark"
	},
	"activationEvents": [
		"onCommand:markdown.generateTable",
		"onCommand:markdown.generateTableWithAlignment"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "markdown.generateTable",
				"title": "Markdown: New Table"
			},
			{
				"command": "markdown.generateTableWithAlignment",
				"title": "Markdown: New Table With Alignment"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.6.1",
		"vscode": "^1.1.6",
		"tslint": "^5.8.0",
		"@types/node": "^7.0.43",
		"@types/mocha": "^2.2.42"
	},
